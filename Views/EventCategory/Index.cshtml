
@model List<Coding_Events.Models.EventCategory>

<h1>Event Categories</h1>


    <table>
        <tr>
            <th>
                Name
            </th>
        </tr>
        @foreach(EventCategory category in Model)
        {
            <tr>
                <td>@category.Name</td>
            </tr>
        }
    </table>
